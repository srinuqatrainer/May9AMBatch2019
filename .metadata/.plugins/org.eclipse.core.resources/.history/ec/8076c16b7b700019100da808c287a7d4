package com.BasicJava;

public class WhileExample {

	public static void main(String[] args) {
		
		int x=1;
		
		while(x<=5)
		{
			System.out.println(x + " - " + "Testing");
			// System.out.println("Testing");
			x++;
		}
		
		System.out.println(" *********** ");
		
		int y=5;
		
		while(y>=1)
		{
			System.out.println(y + " - " + "Testing");
			y--
		}

	}

}
